  object sqsRelatorio: TSQLDataSet [4]
    SchemaName = 'sysdba'
    CommandText = 
      'SELECT'#13#10'VNOTASFISCAIS.CLIENTE,'#13#10'VNOTASFISCAIS.NUM_NFE,'#13#10'VNOTASFI' +
      'SCAIS.DATAEMISSAO,'#13#10'SUM(VINOTASFISCAIS.TOTAL) AS TOTAL'#13#10'FROM VNO' +
      'TASFISCAIS'#13#10'INNER JOIN VINOTASFISCAIS ON (VNOTASFISCAIS.CODIGO =' +
      ' VINOTASFISCAIS.NOTAFISCAL)'#13#10'WHERE VNOTASFISCAIS.STATUS = 0 AND ' +
      'VNOTASFISCAIS.DATAEMISSAO BETWEEN :DATAINI AND :DATAFIM'#13#10'GROUP B' +
      'Y'#13#10'VNOTASFISCAIS.CLIENTE,'#13#10'VNOTASFISCAIS.NUM_NFE,'#13#10'VNOTASFISCAIS' +
      '.DATAEMISSAO'
    DbxCommandType = 'Dbx.SQL'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftDate
        Name = 'DATAINI'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'DATAFIM'
        ParamType = ptInput
      end>
    SQLConnection = DM.SQLConnection
    Left = 200
    Top = 24
  end
