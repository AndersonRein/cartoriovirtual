  object sqsCliente: TSQLDataSet
    SchemaName = 'sysdba'
    CommandText = 
      'SELECT CODIGO, NOME, TELEFONE, DATACADASTRO FROM VCLIENTES WHERE' +
      ' DATACADASTRO BETWEEN :DATAINI AND :DATAFIM'
    DbxCommandType = 'Dbx.SQL'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftDate
        Name = 'DATAINI'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'DATAFIM'
        ParamType = ptInput
      end>
    SQLConnection = DM.SQLConnection
    Left = 128
    Top = 8
  end
