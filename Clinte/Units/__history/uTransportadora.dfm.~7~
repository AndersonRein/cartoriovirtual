    end
  end
  inherited dspCadastro: TDataSetProvider
    DataSet = sqsCadastro
  end
  inherited sqsCadastro: TSQLDataSet
    SchemaName = 'sysdba'
    CommandText = 
      'select CODIGO, RAZAOSOCIAL from VTRANSPORTADORA WHERE RAZAOSOCIA' +
      'L LIKE :NOME'
    Params = <
      item
        DataType = ftString
        Name = 'NOME'
        ParamType = ptInput
      end>
  end
  inherited sqsExcluir: TSQLDataSet
    SchemaName = 'sysdba'
